{
	"name": "domodel-diary-web",
	"description": "Web context for domodel-diary",
	"type": "module",
	"scripts": {
		"generate-docs": "jsdoc --configure .jsdoc.json --verbose",
		"test": "concurrently --success first --raw \"http-server -s -p 8080 dist/prod/public\" --kill-others \"wait-on tcp:8080 && npm run build && mocha\"",
		"start": "rollup --config rollup.config.js --watch --environment BUILD:development",
		"build": "rollup --config rollup.config.js --environment BUILD:production"
	},
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@rollup/plugin-terser": "^0.2.0",
		"concurrently": "^7.6.0",
		"docdash": "^2.0.0",
		"eslint": "^8.30.0",
		"http-server": "^14.1.1",
		"jsdoc": "^4.0.0",
		"mocha": "^10.2.0",
		"postcss": "^8.4.20",
		"postcss-import": "^15.1.0",
		"puppeteer": "^10.2.0",
		"rollup": "^3.7.5",
		"rollup-plugin-copy-watch": "^0.0.1",
		"rollup-plugin-glob-import": "^0.5.0",
		"rollup-plugin-livereload": "^2.0.5",
		"rollup-plugin-postcss": "^4.0.2",
		"rollup-plugin-serve": "^2.0.2",
		"wait-on": "^7.0.1"
	},
	"dependencies": {
		"@domodel/paginator": "^1.0.13",
		"@domodel/popup": "^1.0.13",
		"crypto-es": "^1.2.7",
		"css-generics": "^1.0.8",
		"domodel": "^1.0.46",
		"domodel-diary": "file:../domodel-diary"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/thoughtsunificator/domodel-diary.git"
	},
	"bugs": {
		"url": "https://github.com/thoughtsunificator/domodel-diary/issues"
	},
	"author": "Romain Lebesle <oss@thoughtsunificator.me> (https://thoughtsunificator.me)",
	"license": "MIT",
	"version": "0.0.9"
}
